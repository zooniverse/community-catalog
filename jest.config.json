{
  "moduleNameMapper": {
    "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/jest/mockFile.js",
    "^@src/(.*)$": "<rootDir>/src/$1"
  },
  "roots": [
    "<rootDir>/src"
  ],
  "setupFilesAfterEnv": ["<rootDir>/jest/jest-setup.js"],
  "testEnvironment": "jsdom"
}
